{
  "name": "graphql-to-sparql",
  "version": "1.2.0",
  "description": "Converts GraphQL queries to SPARQL queries",
  "keywords": [
    "graphql",
    "graphqlld",
    "sparql",
    "query",
    "rdf",
    "linked data"
  ],
  "main": "index.js",
  "typings": "index",
  "repository": "git@github.com:rubensworks/graphql-to-sparql.js.git",
  "author": "Ruben Taelman <rubensworks@gmail.com>",
  "bugs": {
    "url": "https://github.com/rubensworks/graphql-to-sparql.js/issues"
  },
  "homepage": "https://github.com/rubensworks/graphql-to-sparql.js#readme",
  "license": "MIT",
  "bin": {
    "graphql-to-sparql": "./bin/graphql-to-sparql.js"
  },
  "files": [
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "bin/**/*.d.ts",
    "bin/**/*.js",
    "index.d.ts",
    "index.js"
  ],
  "pre-commit": [
    "build",
    "lint",
    "test"
  ],
  "devDependencies": {
    "@types/graphql": "^0.13.0",
    "@types/jest": "^23.3.0",
    "@types/minimist": "^1.2.0",
    "@types/rdf-js": "^1.0.1",
    "coveralls": "^3.0.0",
    "jest": "^23.4.1",
    "pre-commit": "^1.2.2",
    "ts-jest": "^23.0.1",
    "tslint": "^5.8.0",
    "tslint-eslint-rules": "^5.3.1",
    "typescript": "^2.6.2"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true,
    "mapCoverage": true
  },
  "scripts": {
    "test": "jest ${1}",
    "test-watch": "jest ${1} --watch",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "tslint index.ts lib/**/*.ts test/**/*.ts --exclude '**/*.d.ts'",
    "build": "tsc",
    "validate": "npm ls",
    "prepare": "npm run build"
  },
  "dependencies": {
    "graphql": "^0.13.2",
    "minimist": "^1.2.0",
    "@rdfjs/data-model": "^1.1.0",
    "sparqlalgebrajs": "^0.7.5"
  }
}
